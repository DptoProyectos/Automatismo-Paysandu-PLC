(*2*)
(* Network 0 *)
(*------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------CANALES ANALÓGICOSLECTURA DEL VALOR DE ENTRADA DEL CANAL ANALÓGICO 0*)
LD    TRUE
CAL    SBR08    %AIW0, AI0_IMin, AI0_IMax, AI0_MMin, AI0_MMax, AI0_Alr, AI0_AlrHig, AI0_AlrLow, PRESION
(* Network 1 *)
(*------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------ENTRADAS DIGITALESSEÑAL DE CAUDALLectura de conteo de pulsos de la entrada digital 0*)
LD    TRUE
CAL    SBR10    %I0.0, DI0_Pton, DI0_Ptoff, DI0_Rst, DI0_Count, DI0_TimePulse
(* Network 2 *)
(*ENTRADAS DIGITALESSEÑAL DE CAUDALCalculo del caudal en l/min: (6 000 000 * magpp)/tp(ms)Qmax = 60 000 000 * mapp L/minQmin = 1831 * mapp   [L/min]*)
LD    TRUE
MOVE    PC_Magpp, real0
MUL    60000000.0, real0
DI_TO_R    DI0_TimePulse, real1
AND(
LD    TRUE
MOVE    real0, CAUDAL_SELLOS
DIV    real1, CAUDAL_SELLOS
OR(
LD    TRUE
GE    real1, 32767.0
MOVE    0.0, CAUDAL_SELLOS
)
)
(* Network 3 *)
LD    TRUE
CAL    SBR10    %I0.1, DI1_Pton, DI1_Ptoff, DI1_Rst, DI1_Count, DI1_TimePulse
